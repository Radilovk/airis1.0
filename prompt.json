  "analysis": {
    "req_id": "Req1/Req2/Req3/Single",
    "zones": [
      {
        "id": 1,
        "name": "сектор/зона по Manual (напр. 2:30 Д или ANW сектор 7:00-8:00)",
        "organ": "орган/система",
        "status": "normal/attention/concern",
        "findings": "кратко значение <60симв",
        "angle": [0,30]
      }
