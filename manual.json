IRIS_RAG_v2={
 scp:"Ирис=рефлексна карта през вегетативната НС; показва генетичен потенциал, конституция, натрупани предразположения. Не е диагноза по медицински имена; цел=превантивен профил, рискове/ресурси, елиминация, възпалителни тенденции, невро-вегетативно напрежение." 

 process:{
  imgQ:"висока резолюция, добър фокус, странично осветление; централна светкавица изглажда релеф и крие влакна." 
  order:"1) imgQ 2) конституционен триас 3) топография 4) знаци 5) синтез+верификация."
 }

 triad:{
  const:{
   LYM:{clr:"син/сив",pred:"вродено по-бавен лимфен дренаж, реактивен имунитет; склонност към алергии, слуз, киселинност, натоварени елим. системи."}
   HEM:{clr:"тъмнокафяв",pred:"ос кръв/метаболизъм; хипотези за дисбаланси в кръвна картина, минерален обмен, функционална уязвимост черен дроб/далак/ендокрин."}
   BIL:{clr:"лешников/зелен/светлокафяв",pred:"смесена; основна уязвимост=храносмилателна ос; черен дроб/жлъчка/панкреас по-ниска ефективност → ензимен дефицит, гликемични колебания."}
  }

  disp:{
   SILK:{fib:"много фини, плътни, прави",pred:"силен вроден резерв, издръжливост; слабост=свръхчувств. НС, висок вегетативен тонус, психосоматика при стрес."}
   LINEN:{fib:"средна плътност, без големи отвори",pred:"балансирана устойчивост."}
   BURLAP:{fib:"рехави, вълнообразни влакна, много лакуни",pred:"вродена слабост на колаген/съединит. тъкан → пролапси, хернии, разширени вени, хронични тъканни проблеми."}
  }

  diath:{
   HAC:{sign:"белезникав/сивкав воал",mean:"тенденция към хиперацидност."}
   LRS:{sign:"верига бели/жълтеникави облачета външен ръб",mean:"лимфен застой/хронична блокада."}
   LIP:{sign:"плътен белезникав пръстен по външния ръб",mean:"липемен метаболизъм ↓, СС натоварване."}
   DYS:{sign:"тъмни мръсни оцветявания",mean:"метаболитно натоварване на кръв/черен дроб."}
  }
 }

